	"version": "0.2.0",
	"configurations": [
		{
			"cwd": "${workspaceFolder}",
			"executable": "${workspaceRoot}/build/kl05z-sample-project.elf",
			"name": "Debug with JLink",
			"request": "launch",
			"type": "cortex-debug",
			"serverpath": "/usr/bin/JLinkGDBServerCLExe",
			"armToolchainPath": "/usr/bin",
			"device": "MKL05Z32xxx4",
